<resources>

    <!-- App error dialog -->
    <java-symbol type="id" name="aerr_copy" />
    <java-symbol type="string" name="url_copy_success" />
    <java-symbol type="string" name="url_copy_failed" />

    <!-- Vendor mismatch fingerprint warning -->
    <java-symbol type="string" name="system_error_vendorprint" />

    <!-- Whether or not we should show vendor mismatch message -->
    <java-symbol type="bool" name="config_show_vendor_mismatch_message" />

    <!-- Package Manager -->
    <java-symbol type="array" name="config_deviceDisabledComponents" />
    <java-symbol type="array" name="config_globallyDisabledComponents" />
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Vendor signatures -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- Device keyhandlers -->
    <java-symbol type="array" name="config_deviceKeyHandlerLibs" />
    <java-symbol type="array" name="config_deviceKeyHandlerClasses" />

    <!-- Power menu reboot -->
    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_confirm" />
    <java-symbol name="reboot_to_bootloader_title" type="string" />
    <java-symbol name="reboot_to_bootloader_message" type="string" />
    <java-symbol name="reboot_to_fastboot_title" type="string" />
    <java-symbol name="reboot_to_fastboot_message" type="string" />
    <java-symbol name="reboot_to_recovery_title" type="string" />
    <java-symbol name="reboot_to_recovery_message" type="string" />
    <java-symbol name="reboot_system_title" type="string" />
    <java-symbol name="reboot_system_message" type="string" />
    <java-symbol type="array" name="config_rebootActionsList" />

    <!-- Whether to cleanup fingerprints upon connection to the daemon and when user switches -->
    <java-symbol type="bool" name="config_cleanupUnusedFingerprints" />
    
    <!-- Path to fast charging status file to detect whether an oem fast charger is active -->
    <java-symbol type="string" name="config_oemFastChargerStatusPath" />

    <!-- Expected value from fast charging status file -->
    <java-symbol type="string" name="config_oemFastChargerStatusValue" />

    <!-- SystemUI Restart -->
    <java-symbol type="string" name="systemui_restart_title" />
    <java-symbol type="string" name="systemui_restart_message" />

    <!-- Whether device supports an alt. ambient display -->
    <java-symbol type="bool" name="config_alt_ambient_display"/>

    <!-- AlertSlider modes -->
    <java-symbol type="string" name="alert_slider_mode_normal" />
    <java-symbol type="string" name="alert_slider_mode_priority" />
    <java-symbol type="string" name="alert_slider_mode_vibrate" />
    <java-symbol type="string" name="alert_slider_mode_silent" />
    <java-symbol type="string" name="alert_slider_mode_dnd" />

    <!-- Whether device has hardware alertslider -->
    <java-symbol type="bool" name="config_hasAlertSlider" />

    <!-- Hide IME space -->
    <java-symbol type="dimen" name="navigation_bar_frame_height_hide_ime" />
    <java-symbol type="dimen" name="navigation_bar_frame_height_landscape_hide_ime" />

    <!-- Pocket Lock -->
    <java-symbol type="layout" name="pocket_lock_view" />

    <!-- Pocket bridge -->
    <java-symbol type="string" name="config_pocketBridgeSysfsInpocket" />
    <java-symbol type="bool" name="config_pocketModeSupported" />

    <!-- Pocket Service -->
    <java-symbol type="string" name="config_pocketJudgeVendorSensorName" />

